                <span class="color-label">Black</span>
                <div class="route-mini-preview">
                  <div class="mini-preview-map" id="mini-preview-black"></div>
                </div>
              </button>
              <!-- White -->
              <button class="route-color-option" data-color="#ffffff" aria-label="White route color">
                <span class="color-swatch" style="background: #ffffff; border: 1px solid #e0e0e0;"></span>
                <span class="color-label">White</span>
