{
  "name": "map-printing-backend",
  "version": "1.0.0",
  "description": "Backend server for Shopify Custom Map Printing Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "tunnel": "ngrok http --url=boss-hog-freely.ngrok-free.app 3000",
    "capture-ngrok": "node scripts/capture-ngrok.js",
    "tunnel:capture": "concurrently --kill-others-on-fail \"npm run tunnel\" \"wait-on http://localhost:4040 && npm run capture-ngrok\"",
    "dev:full": "concurrently \"npm run dev\" \"npm run tunnel:capture\"",
    "dev:complete": "concurrently --kill-others-on-fail \"npm run tunnel\" \"wait-on http://localhost:4040 && npm run capture-ngrok && npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "shopify",
    "maps",
    "printing",
    "strava",
    "mapbox"
  ],
  "author": "Print My Ride",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.4.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "wait-on": "^7.2.0"
  }
}
