# Shopify Map Printing Platform - Environment Configuration
# Copy this file to .env and fill in your actual values

# Strava API Configuration
STRAVA_CLIENT_ID=your_strava_client_id_here
STRAVA_CLIENT_SECRET=your_strava_client_secret_here
STRAVA_REDIRECT_URI=https://your-ngrok-url.ngrok.io/auth/strava/callback

# Mapbox Configuration
MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# Session Configuration
SESSION_SECRET=your_secure_random_string_here

# Server Configuration
NODE_ENV=development
PORT=3000

# Shopify Configuration (when implemented)
SHOPIFY_API_KEY=your_shopify_api_key_here
SHOPIFY_SECRET_KEY=your_shopify_secret_key_here
SHOPIFY_STORE_URL=print-my-ride-version-5.myshopify.com

# Development Configuration
DEBUG=*
LOG_LEVEL=debug